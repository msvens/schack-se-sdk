{
  "name": "@msvens/schack-se-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for the Swedish Chess Federation (schack.se) API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts src/services/index.ts src/types/index.ts src/utils/index.ts --format cjs,esm && tsc --emitDeclarationOnly --outDir dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "rm -rf dist && tsup src/index.ts src/services/index.ts src/types/index.ts src/utils/index.ts --format cjs,esm && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "chess",
    "schack",
    "ssf",
    "swedish-chess",
    "api",
    "sdk"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/msvens/schack-se-sdk.git"
  },
  "author": "Martin Svensson"
}
